.result_container{
    background-color: #B1F3E4;
}
/*swiper_container*/
.swiper_container{
    position: relative;
    width: 100%;
    height: 1020rpx;
    box-sizing: border-box;
}
.readingItem{
    padding: 44rpx;
    box-sizing: border-box;
}
/*readingItem-image*/
.readingItem-img{
    width: 100%;
    height: 656rpx;
    box-sizing: border-box;
    border:4rpx solid #452317;
    border-radius: 10rpx;
}
/*readingItem-content*/
.readingItem-content{
    display: flex;
    flex-flow:column wrap;
    align-items: center;
    justify-content: center;
    margin-top: 20rpx;
    padding: 10rpx 30rpx 40rpx 30rpx;
    background-color: #FCAEA0;
    border:4rpx solid #452317;
    border-radius: 10rpx;
}
.item-index{
    margin-bottom: 16rpx;
    font-size: 32rpx;
}
/* readingItem-audio-content */
.readingItem-audio-content{
    display: flex;
    flex-flow:row nowrap;
    justify-content: flex-start;
    height: 100rpx;
    width: 100%;
}
.read-audio-icon{
    display: flex;
    width: 100rpx;
    height: 100rpx;
    flex-shrink: 0;
    margin-right: 16rpx;
}

/* read-audio-info */
.read-audio-info{
    display: flex;
    flex-flow:column nowrap;
    justify-content: center;
    align-items: flex-start;
    font-size: 28rpx;
}
.ri-en_content{

}
.ri-content{
    color: #FFFFFF;
}
/*read-audio-control*/
.read-audio-control{
    display: flex;
    flex-flow:row nowrap;
    align-items: center;
    width: 100%;
    height: 70rpx;
}

.read-audio-progress{
    width: 320rpx;
}
.read-audio-duration{
    margin-left: 20rpx;
    font-size: 32rpx;
}
.audio-teacher-intro{
    width: 184rpx;
    height: 56rpx;
    margin-left: 30rpx;
}

/* swiper 下面的附加信息 */
.plugin_info_container{
    position: relative;
    width: 100%;
    padding: 0 40rpx 80rpx 40rpx;
    box-sizing: border-box;
}
/* 老师 点评 */
.teacher-comment{
    position: relative;
    flex-flow:column wrap;
    width: 100%;
    margin-top: 92rpx;
    box-sizing: border-box;
    border: 4rpx solid #462518;
    border-radius: 10rpx;
    padding-top: 40rpx;
    background-color: #FFFFFF;
}
.teacher-comment-icon{
    position: absolute;
    left: 200rpx;
    top: -40rpx;
    width: 280rpx;
    height: 84rpx;
}
.teacher-comment-content{
    font-size: 28rpx;
    padding: 20rpx 20rpx 52rpx 20rpx;
    text-indent: 2em;
}

/* 他人 跟读记录 */
/*record-list*/
.record-list{
    position: relative;
    display: flex;
    flex-flow:column wrap;
    margin-top: 60rpx;
    min-height: 5rem;
    box-sizing: border-box;
    border: 4rpx solid #462518;
    border-radius: 10rpx;
    padding-top: 40rpx;
    background-color: #FFFFFF;
}
.record-list-icon{
    position: absolute;
    left: 200rpx;
    top: -40rpx;
    width: 280rpx;
    height: 84rpx;
}
.default-record-text{
    display: flex;
    height: 3rem;
    justify-content: center;
    align-items: center;
}
/*recordItem*/
.recordItem{
    position: relative;
    display: flex;
    flex-flow:row wrap;
    height: 110rpx;
    background-color: #FFFFFF;
    border-bottom: 2rpx solid #EFEFEF;
    border-radius: 10rpx;
    padding: 25rpx 40rpx;
}
/*recordItem-portrait*/
.recordItem-portrait{
    width: 60rpx;
    height: 60rpx;
    background-color: #B1F3E4;
    border-radius: 50%;
}

.recordItem-info{
    display: flex;
    flex-flow:column wrap;
}
.recordItem-uname{
    margin-left: 26rpx;
}

.recordItem-audio{
    position: relative;
    display: flex;
    margin-left: 6rpx;
    width: 280rpx;
    height: 70rpx;
    justify-content: space-between;
    align-items: center;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: contain;
}
.recordItem-audio-bg{
    position: absolute;
    width: 280rpx;
    height: 70rpx;
}

.recordItem-audio-icon{
    margin-left: 40rpx;
    width: 24rpx;
    height: 32rpx;
    z-index: 9;
}

/* recordItem-time */
.recordItem-time{
    position: absolute;
    top: 20rpx;
    right: 40rpx;
    color: #BDBDBD;
}