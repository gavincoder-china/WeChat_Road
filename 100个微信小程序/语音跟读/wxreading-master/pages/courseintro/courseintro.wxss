.csintro_container{
    background: #B1F3E4;
    padding: 36rpx 40rpx 160rpx 40rpx;
}
.csintro-header{
  display: flex;
  flex-direction: column;
  justify-content:center;
  align-items:center;
  width: 100%;
  height: 376rpx;
  background-color: silver;
  box-sizing: border-box;
  border: 4rpx solid #462518;
  border-radius: 10rpx;
}
.csih-content{
  width: 76%;
  font-size: 28rpx;
  text-align: center;
  color: #FFFFFF;
}
.csi-play-btn{
  width: 100rpx;
  height: 100rpx;
}

/*csintro-article*/
.csintro-article{
  display: flex;
  width: 100%;
  flex-flow:column wrap;
}
.csiart-title{
  display: flex;
  line-height: 80rpx;
  font-size: 28rpx;
  margin-left: 24rpx;
}
.word-list{
  display: flex;
  flex-flow:row wrap;
  justify-content: space-around;
}

.word-item {
    position: relative;
    display: flex;
    flex-flow:row wrap;
    justify-content: center;
    align-items: center;
    width: 280rpx;
    height: 60rpx;
    margin-bottom: 30rpx;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: contain;
}

.word-item-text{
  margin-right: 10rpx;
}
.word-item-play{
    width: 28rpx;
    height: 28rpx;
}

/*record-list*/
.record-list{
    position: relative;
    flex-flow:column wrap;
    width: 100%;
    min-height: 5rem;
    margin-top: 92rpx;
    box-sizing: border-box;
    border: 4rpx solid #462518;
    border-radius: 10rpx;
    padding-top: 40rpx;
    background-color: #FFFFFF;
}
.record-list-icon{
    position: absolute;
    left: 200rpx;
    top: -40rpx;
    width: 280rpx;
    height: 84rpx;
}
.default-record-text{
    display: flex;
    height: 3rem;
    justify-content: center;
    align-items: center;
}
/*recordItem*/
.recordItem{
    position: relative;
    display: flex;
    flex-flow:row wrap;
    height: 110rpx;
    background-color: #FFFFFF;
    border-bottom: 2rpx solid #EFEFEF;
    border-radius: 10rpx;
    padding: 25rpx 40rpx;
}
/*recordItem-portrait*/
.recordItem-portrait{
    width: 60rpx;
    height: 60rpx;
    background-color: #B1F3E4;
    border-radius: 50%;
}

.recordItem-info{
    display: flex;
    flex-flow:column wrap;
}
.recordItem-uname{
    margin-left: 26rpx;
}

.recordItem-audio{
    position: relative;
    display: flex;
    margin-left: 6rpx;
    width: 280rpx;
    height: 70rpx;
    justify-content: space-between;
    align-items: center;
    background: url("data:image/png;base64,") no-repeat center center;
    background-size: contain;
}
.recordItem-audio-bg{
    position: absolute;
    width: 280rpx;
    height: 70rpx;
}

.recordItem-audio-icon{
    margin-left: 40rpx;
    width: 24rpx;
    height: 32rpx;
}

/* recordItem-time */
.recordItem-time{
    position: absolute;
    top: 20rpx;
    right: 40rpx;
    color: #BDBDBD;
}

/*fixed_study_btn*/
.fixed_study_btn{
    position: fixed;
    display: flex;
    justify-content: center;
    align-items: center;
    left: 0;
    bottom: 0;
    width: 100%;
    height: 100rpx;
    background-color: #1ECFA7;
    font-size: 28rpx;
    color: #FFFFFF;
}