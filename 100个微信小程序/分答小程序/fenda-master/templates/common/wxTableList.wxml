<!--
  伪造类iOS tableView
  index: int
  msg: string
  time: string
-->
<template name="wxTableList">
    <view>
        <!--wxTableListTop
        - 放置上拉刷新等
        -->
        <view class="wxTableListTop">
            wxTableListTop
        </view>

        <view class="wxTableListHeader">
            {{test}}
        </view>

        <view class="wxTableListBody">
            <view class="wxTableListPlainStyle">

               <!--分组1-->
               <view class="weui-cells__title">带说明的列表项</view>
               <view class="weui-cells weui-first-cells">
                    <view class="weui-cell">
                        <view class="weui-cell__bd">
                            <text>标题文字</text>
                        </view>
                        <view class="weui-cell__ft">说明文字</view>
                    </view>
               </view>
               <!--分组2-->
               <view class="weui-cells__title">带图标、说明的列表项</view>
                <view class="weui-cells  weui-first-cells">
                        <view class="weui-cell">
                            <view class="weui-cell__hd">
                                <image class="weui-cell__icon" src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block" />
                            </view>
                            <view class="weui-cell__bd">
                                <text>标题文字</text>
                            </view>
                            <view class="weui-cell__ft">说明文字</view>
                        </view>
                        <view class="weui-cell">
                            <view class="weui-cell__hd">
                                <image class="weui-cell__icon" src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block" />
                            </view>
                            <view class="weui-cell__bd">
                                <text>标题文字</text>
                            </view>
                            <view class="weui-cell__ft">说明文字</view>
                        </view>
                    </view>
                    <!--分组三-->
                    <view class="weui-cells__title">带跳转的列表项</view>
                    <view class="weui-cells">
                        <view class="weui-cell weui-cell_access">
                            <view class="weui-cell__bd">
                                <text>cell standard</text>
                            </view>
                            <view class="weui-cell_access__ft">
                            </view>
                        </view>
                        <view class="weui-cell weui-cell_access">
                            <view class="weui-cell__bd">
                                <text>cell standard</text>
                            </view>
                            <view class="weui-cell_access__ft">
                            </view>
                        </view>
                    </view>

                    <!--disan-->
                    <view class="weui-cells__title">带图标、说明、跳转的列表项</view>
                    <view class="weui-cells">
                        <view class="weui-cell weui-cell_access">
                            <view class="weui-cell__hd">
                                <image class="weui-cell__icon" src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block" />
                            </view>
                            <view class="weui-cell__bd">
                                <text>cell standard</text>
                            </view>
                            <view class="weui-cell_access__ft">说明文字</view>
                        </view>
                        <view class="weui-cell weui-cell_access">
                            <view class="weui-cell__hd">
                                <image class="weui-cell__icon" src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block" />
                            </view>
                            <view class="weui-cell__bd">
                                <text>cell standard</text>
                            </view>
                            <view class="weui-cell_access__ft">说明文字</view>
                        </view>
                    </view>


            </view>

            <view class="wxTableListGroupStyle">
               
            </view>

        </view>

        <view class="wxTableListfooter">
            wxTableListfooter
        </view>

        <!--wxTableListTop
        - 放置下拉加载等
        -->
        <view class="wxTableListBottom">
            wxTableListBottom
        </view>
    </view>
</template>